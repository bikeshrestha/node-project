            "image":"bikeshrestha/node-project:$COMMIT_HASH",
            "hostname":"node-project",
            "essential":true,
            "portMappings":[
                {
                    "hostPort":80,
                    "containerPort":8080
                }
            ],
            "memory":400
        }
    ]
}