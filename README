
#BUILD
docker build -t <your username>/node-project:$(git log -1 --format=%h) .

#RUN
docker run -p 49160:8080 -d <your username>/node-project

#PUSH
docker login
docker tag <your username>/node-project:$(git log -1 --format=%h) <your username>/node-project:latest
docker push <your username>/node-project
